p
    h1
        = @video.name

.row
    .col-md-12
        p Name: #{@video.name}
        p URL: #{@video.url}
        p
            iframe width="420" height="315" src="#{@video.url}" frameborder="0" allowfullscreen
            
h5
    = link_to "Return to videos listing", videos_path, class: "btn btn-warning btn-small"