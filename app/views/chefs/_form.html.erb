<%= render 'shared/errors', obj: @chef %>
 
<div class="row">
    <div class="well col-md-8 col-md-offset-2">
        <%= form_for @chef do |f| %>
            <%= f.label :chefname %>
            <%= f.text_field :chefname %>
            
            <%= f.label :email %>
            <%= f.text_field :email %>
           
            <%= f.submit(@chef.new_record? ? 'Create chef' : 'Edit chef', class: "btn btn-success") %>
        <% end %>
    </div>
</div>